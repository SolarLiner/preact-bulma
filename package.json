{
  "name": "preact-bulma",
  "version": "1.0.0a",
  "description": "Preact components for the Bulma framework",
  "main": "lib/preact-bulma.js",
  "umd:main": "lib/preact-bulma.umd.js",
  "module": "lib/preact-bulma.mjs",
  "source": "src/entry.module.ts",
  "typings": "lib/preact-bulma.d.ts",
  "author": "Nathan Graule",
  "license": "MIT",
  "private": false,
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "scripts": {
    "build": "microbundle build",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "docs": "build-storybook -c .storybook -o docs",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prepack": "microbundle build"
  },
  "peerDependencies": {
    "preact": "^10.0.0-alpha.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-knobs": "^5.0.0",
    "@storybook/addon-options": "^5.0.0",
    "@storybook/addon-storysource": "^5.0.0",
    "@storybook/preact": "^5.0.0",
    "@types/classnames": "^2.2.7",
    "@types/node": "^11.10.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-preact": "tomasdev/babel-preset-preact#patch-1",
    "dts-bundle": "^0.7.3",
    "microbundle": "^0.9.0",
    "node-sass": "^4.9.3",
    "preact": "^10.0.0-alpha.0",
    "preact-compat": "^3.18.4",
    "prettier": "^1.15.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.0.3",
    "webpack": "^4.19.0"
